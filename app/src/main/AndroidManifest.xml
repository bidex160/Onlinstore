<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.store">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/sics"
        android:label="SicoCosmetics"
        android:roundIcon="@drawable/sico"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name=".CompletedOrder"></activity>
        <activity
            android:name=".Checkout"
            android:label="SicoCosmetics" />
        <activity
            android:name=".Cartactivity"
            android:label="Sico Cosmetics" />
        <activity
            android:name=".SecondActivity"
            android:label="SicoCosmetics" />
        <activity
            android:name=".AddData"
            android:label="SicoCosmetics " />
        <activity
            android:name=".MainActivity"
            android:label="@string/cname">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>